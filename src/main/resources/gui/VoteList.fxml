                  </VBox>
               </content>
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </ScrollPane>
            <AnchorPane prefHeight="39.0" prefWidth="300.0">
               <children>
                  <Button fx:id="voteButton" layoutY="12.0" mnemonicParsing="false" onAction="#vote" text="Vote" />
