<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="mainBox" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kamil.VoteCalculator.gui.VoteList">
   <children>
      <ScrollPane prefHeight="381.0" prefWidth="360.0">
         <content>
            <VBox fx:id="voteBox">
               <padding>
                  <Insets left="50.0" />
               </padding>
               <opaqueInsets>
                  <Insets top="3.0" />
               </opaqueInsets>
            </VBox>
         </content>
      </ScrollPane>
      <Button fx:id="voteButton" mnemonicParsing="false" onAction="#vote" text="Vote" />
   </children>
</VBox>
